{
  "name": "url-shortener",
  "version": "1.0.0",
  "description": "A url shortener built with GraphQL",
  "main": "index.js",
  "scripts": {
    "test": "cross-env DATABASE_URL=$TEST_DATABASE_URL jest --forceExit",
    "dev": "node -r esm src/",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "prestart": "npm run db:migrate && npm run db:generate",
    "build": "babel src/ --out-dir dist --copy-files",
    "start": "node dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MlkMahmud/url-shortener.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MlkMahmud/url-shortener/issues"
  },
  "homepage": "https://github.com/MlkMahmud/url-shortener#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@types/jest": "^26.0.22",
    "@types/supertest": "^2.0.11",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "esm": "^3.2.25",
    "jest": "^26.6.3",
    "prisma": "^2.21.2",
    "supertest": "^6.1.3"
  },
  "dependencies": {
    "@prisma/client": "^2.21.2",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.4",
    "is-url": "^1.2.4"
  }
}
